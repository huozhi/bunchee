{
  "name": "bunchee",
  "version": "1.6.0",
  "description": "zero config bundler for js/ts/jsx libraries",
  "bin": {
    "bunchee": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf ./dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn clean && yarn build && chmod +x dist/cli.js && yarn test",
    "build:cli": "node -r ts-node/register src/cli.ts src/cli.ts --target node -f cjs -o dist/cli.js",
    "build:main": "node -r ts-node/register src/cli.ts src/index.ts --target node -f cjs",
    "build": "yarn build:main && yarn build:cli"
  },
  "type": "commonjs",
  "keywords": [
    "bundler",
    "javascript",
    "bundle",
    "typescript",
    "jsx",
    "zero-config",
    "library"
  ],
  "files": [
    "dist",
    "*.md"
  ],
  "author": "huozhi (github.com/huozhi)",
  "license": "MIT",
  "dependencies": {
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-json": "4.0.2",
    "@rollup/plugin-node-resolve": "11.2.1",
    "@rollup/plugin-typescript": "8.2.0",
    "arg": "5.0.0",
    "babel-preset-o": "0.4.1",
    "rollup": "1.32.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-preserve-shebang": "1.0.1",
    "rollup-plugin-terser": "7.0.2",
    "tslib": "2.3.0",
    "typescript": "^3.9.6"
  },
  "peerDependencies": {
    "typescript": ">= 3.7.0"
  },
  "optionalDependencies": {
    "typescript": ">= 3.7.0"
  },
  "devDependencies": {
    "@types/node": "14.14.32",
    "jest": "26.6.1",
    "ts-node": "10.0.0"
  }
}
